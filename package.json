{
  "name": "kreia",
  "version": "0.3.2",
  "description": "A flexible and easy to use parser library. Look into chaos, and see order.",
  "main": "./kreia.js",
  "types": "./kreia.d.ts",
  "scripts": {
    "main": "ts-node lib/IndentationLexer.ts",
    "preversion": "npm test",
    "finalize-version": "git push origin master && git push origin master --tags && npm publish",
    "test": "mocha -r ts-node/register lib/ast.spec.ts",
    "ts:ast": "ts-creator lib/generate_from.ts --use-tabs true --trailing-comma 'all' -o lib/generate_to.ts"
  },
  "keywords": [
    "parser",
    "generator",
    "lookahead",
    "automatic"
  ],
  "author": {
    "name": "Blaine Hansen",
    "email": "faichenshing@gmail.com",
    "url": "http://blainehansen.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blainehansen/kreia"
  },
  "license": "MIT",
  "devDependencies": {
    "@ts-std/enum": "^0.1.2",
    "@ts-std/types": "^1.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.12",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "ts-creator": "^1.2.5",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@ts-std/collections": "^1.0.6",
    "@ts-std/extensions": "^1.0.10",
    "@ts-std/monads": "^1.0.4",
    "kreia-moo": "^0.4.8"
  }
}
