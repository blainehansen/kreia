{
  "name": "kreia",
  "version": "0.3.2",
  "description": "A flexible and easy to use parser library. Look into chaos, and see order.",
  "main": "./kreia.js",
  "types": "./kreia.d.ts",
  "scripts": {
    "main": "ts-node lib/compiler/grammar_ast.ts",
    "parse": "ts-node lib/compiler/grammar_out.ts",
    "preversion": "npm test",
    "finalize-version": "git push origin master && git push origin master --tags && npm publish",
    "test": "mocha -r ts-node/register lib/**/*.spec.ts lib/*.spec.ts",
    "ts:ast": "ts-creator lib/compiler/_generate_from.ts --use-tabs true --trailing-comma 'all' -o lib/compiler/_generate_to.ts"
  },
  "keywords": [
    "parser",
    "generator",
    "lookahead",
    "automatic"
  ],
  "author": {
    "name": "Blaine Hansen",
    "email": "faichenshing@gmail.com",
    "url": "http://blainehansen.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blainehansen/kreia"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.22",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-creator": "^1.2.5",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@ts-std/collections": "^1.1.1",
    "@ts-std/extensions": "^1.1.0",
    "@ts-std/monads": "^1.0.4",
    "@ts-std/types": "^1.0.1"
  }
}
